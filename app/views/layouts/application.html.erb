<!DOCTYPE html>
<html>
  <head>
    <title>VueOnRails</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_packs_with_chunks_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_packs_with_chunks_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_packs_with_chunks_tag 'main', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'navigation', 'data-turbolinks-track': 'reload'  %>
  </head>
  <body>
    <div class="container">
      <%= yield %>
    </div>
  </body>
</html>
