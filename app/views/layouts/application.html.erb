<!DOCTYPE html>
<html>
  <head>
    <title>VueOnRails</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'main', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <div class="container" data-behavior="vue-app">
      <v-app>
        <v-container>
          <v-row>
            <v-col cols="12">
              <Navigation></Navigation>
            </v-col>
          </v-row>
          <v-row>
            <v-col cols="3">
              <Layout></Layout>
            </v-col>
            <v-col cols="9">
              <User></User>
            </v-col>
          </v-row>
        </v-container>
      </v-app>
    </div>
  </body>
</html>
